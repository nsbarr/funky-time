<div id="rollcall">
    <% @user.each do |i| %>
    <ul><%= i.id %> 
        <li><%= i.phone %></li>
        <li><%= i.prompt %></li>
        <% if i.bid == nil%>
            <li class = "notdone">needs poem!<a href="/users/<%=i.id%>/edit">write it</li>
        <% else %>
            <li class = "done">done</li>
        <% end %>
    </ul>
    <% end %>
</div>